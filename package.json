{
  "name": "new-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:cloudflare": "opennextjs-cloudflare build",
    "build:staging": "NODE_ENV=production next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:analyze": "node scripts/test-coverage-report.js",
    "test:full-report": "npm run test:coverage && npm run test:analyze",
    "test:monitor": "node scripts/coverage-monitor.js",
    "test:monitor:watch": "node scripts/coverage-monitor.js --watch",
    "test:monitor:analyze": "node scripts/coverage-monitor.js --analyze",
    "test:unit": "jest --testPathPatterns=src",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:accessibility": "playwright test tests/accessibility.spec.ts",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "cf-logs": "wrangler tail",
    "cf-analytics": "wrangler analytics",
    "deploy:staging": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env staging",
    "deploy:production": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env production",
    "env:check": "node scripts/check-env.js",
    "env:setup:staging": "./scripts/setup-env.sh staging",
    "env:setup:production": "./scripts/setup-env.sh production",
    "security:check": "npm run env:check && git status --ignored | grep -E '\\.env|\\.open-next'",
    "create:test-user": "node scripts/create-supabase-user.mjs"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.8.3",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@cloudflare/next-on-pages": "^1.13.16",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^9.1.7",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wrangler": "^4.38.0"
  }
}
